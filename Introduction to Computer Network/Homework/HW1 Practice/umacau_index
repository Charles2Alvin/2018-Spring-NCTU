<script language="javascript">
    function mouseOverDisplay(id, e) {
        var target = $("#id-"+id);
        var htmlWidth = $(document).width();
        if(htmlWidth >=767){
            target.show();
            $(e).addClass("mouseIn");
            //e.style.background = "rgba(86,140,208,0.9)";
        }
    }
    function mouseOutHide(id, e) {
        var target =$("#id-"+id);  //update by ray
        var htmlWidth = $(document).width();
        if(htmlWidth >=767){
            target.hide();
            $(e).removeClass("mouseIn");
            //e.style.background = "#002a60";

        }
    }
    function mouseClickDisplay(id,e){

        var target = $("#id-"+id);
        var htmlWidth = $(document).width();
        if(htmlWidth <767){
            var targets = $(".sub-bigmenu");
            var targets2 = $(".bigmenu-bt");
            targets.each(function(e){
                if ($(this).attr('id') != target.attr('id'))
                {
                    $(this).hide().removeClass("mouseIn");
                }
            });
			targets2.each(function(e){
                if ($(this).attr('id') != target.attr('id'))
                {
                    $(this).removeClass("mouseIn");
                }
            });


            if (target.css('display')=='none') {
                target.show();
                $(e).addClass('mouseIn');
            }else
            {
                target.hide();
                $(e).removeClass('mouseIn');
            }
        }
    }
    function mouseOverDisplay1(id) {
        var target = $("#id-"+id);
        target.show();
        var e = $("#a_id-"+id);
        e.addClass('mouseIn');
        //e.style.background = "rgba(86,140,208,0.9)";
    }
    function mouseOutHide1(id) {
        var target = $("#id-"+id);
        target.hide();
        var e = $("#a_id-"+id);
        target.removeClass("mouseIn");
		e.removeClass("mouseIn");
        //e.style.background = "#002a60";
    }
    window.onload=function(){
        var htmlWidth = $(document).width();
        if(htmlWidth <767){
            var targets = document.getElementsByClassName("bigmenu-bt");
            for(var i = 0; i<targets.length; i++){
                targets[i].href="#";
            }
        }
    }
    window.onresize=function(){
        var htmlWidth = $(document).width();
        if(htmlWidth <767){
            var targets = document.getElementsByClassName("bigmenu-bt");
            for(var i = 0; i<targets.length; i++){
                targets[i].href="#";
            }
        } else {
                        var targets = document.getElementsByClassName("bigmenu-bt");
            for(var i = 0; i<targets.length; i++){
                if(targets[i].id=="a_id-71"){
                    targets[i].href="about-um.html"/*tpa=http://www.umac.mo/about-um.html*/;
                } else if(targets[i].id=="a_id-70"){
                    targets[i].href="study-at-um.html"/*tpa=http://www.umac.mo/study-at-um.html*/;
                } else if(targets[i].id=="a_id-69"){
                    targets[i].href="research.html"/*tpa=http://www.umac.mo/research.html*/;
                }  else  {
                    targets[i].href="#123456";
                }
            }
        }
    }
</script>
<div class="sub-bigmenu-bg" style="width:100%;position:absolute;z-index:300">
    <div class="container">
        <div class="s-sub-bigmenu">
