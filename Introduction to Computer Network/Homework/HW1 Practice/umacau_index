,"2014-07-07","2014-07-20","2014-07-23","2014-07-24","2014-07-26","2014-07-27","2014-08-01","2014-08-02","2014-08-03","2014-08-07","2014-08-08","2014-08-10","2014-08-24","2014-08-31","2014-09-06","2014-09-14","2014-09-18","2014-09-21","2014-10-01","2014-10-02","2014-10-03","2014-11-15","2014-11-16","2014-12-21","2014-12-28","2015-01-04","2015-01-11","2015-03-22","2015-05-02","2015-05-03","2016-06-28","2016-06-29","2016-07-01","2016-07-03","2016-08-06","2016-08-07","2016-08-20","2016-08-21","2016-08-28","2016-09-11","2016-10-01","2016-10-02","2016-10-03","2016-10-04","2017-04-29","2017-04-30","2017-05-01","2017-07-01","2017-08-05","2017-08-06","2017-08-20","2017-08-26","2017-08-27","2017-08-31","2017-09-10","2017-10-01","2017-10-02","2017-10-03","2017-10-04","2017-10-05","2017-10-08","2018-06-28","2018-06-29","2018-06-30","2018-07-01","2018-07-02","2018-07-03","2018-07-04","2018-07-05","2018-07-06","2018-07-07","2018-07-08","2018-07-09","2018-07-10","2018-07-11","2018-07-12","2018-07-13","2018-07-14","2018-07-15","2018-07-16","2018-07-17","2018-07-18","2018-07-19","2018-07-20","2018-07-21","2018-07-22","2018-07-23","2018-07-24","2018-07-25","2018-07-26","2018-07-27","2018-07-28","2018-07-29","2018-07-30","2018-07-31","2018-08-01","2018-08-02","2018-08-03","2018-08-04","2018-08-05","2018-08-06","2018-08-07","2018-08-08","2018-08-09","2018-08-10","2018-08-11","2018-08-12","2018-08-13","2018-08-14","2018-08-15","2018-08-16","2018-08-17","2018-08-18","2018-08-19","2018-08-20","2018-08-21","2018-08-22","2018-08-23","2018-08-24","2018-08-25","2018-08-26","2018-08-27","2018-08-28","2018-08-29","2018-08-30","2018-08-31","2018-09-01","2018-09-02","2018-09-03","2018-09-04","2018-09-05","2018-09-06","2018-09-07","2018-09-08","2018-09-09","2018-09-10","2018-09-11","2018-09-12","2018-09-13","2018-09-14","2018-09-15","2018-09-16","2018-09-17","2018-09-18","2018-09-19","2018-09-20","2018-09-21","2018-09-22","2018-09-23","2018-09-24","2018-09-25","2018-09-26","2018-09-27","2018-09-28","2018-09-29","2018-09-30","2018-10-01","2018-10-02","2018-10-03","2018-10-04","2018-10-05","2018-10-06","2018-10-07","2018-10-08","2018-10-09","2018-10-10","2018-10-11","2018-10-12","2018-10-13","2018-10-14","2018-10-15","2018-10-16","2018-10-17","2018-10-18","2018-10-19","2018-10-20","2018-10-21","2018-10-22","2018-10-23","2018-10-24","2018-10-25","2018-10-26","2018-10-27","2018-10-28","2018-10-29","2018-10-30","2018-10-31","2018-11-01","2018-11-02","2018-11-03","2018-11-04","2018-11-05","2018-11-06","2018-11-07","2018-11-08","2018-11-09","2018-11-10","2018-11-11","2018-11-12","2018-11-13","2018-11-14","2018-11-15","2018-11-16","2018-11-17","2018-11-18","2018-11-19","2018-11-20","2018-11-21","2018-11-22","2018-11-23","2018-11-24","2018-11-25","2018-11-26","2018-11-27","2018-11-28","2018-11-29","2018-11-30","2018-12-01"];
        $('.home_calander').datepicker(
            {
                startDate: new Date('2003-03-04'),
                endDate: new Date('2018-12-01'),
                todayHighlight: true,
                language: "en-GB",
                default: 'yyyy/mm/dd',
                beforeShowDay: function (date) {

                    var formattedDate = new Date(date);

                    if ($.inArray(getDateString(formattedDate), disabled_dates) != -1) {

                        return {
                            enabled: false
                        };
                    }
                    return;
                }
            }).on('changeDate', function (e) {
            date = e.date;
            var y = date.getFullYear();
            var m = date.getMonth() + 1;
            var d = date.getDate();
            if (m < 10 || m.length < 2)
                m = "0" + m;
            if (d < 10 || d.length < 2)
                d = "0" + d;

            window.location.href = 'news-centre/news-and-events/event-calendar/index.htm'/*tpa=http://www.umac.mo/news-centre/news-and-events/event-calendar/*/ + y + '' + m + '' + d + '/';
        });
    });
</script>


